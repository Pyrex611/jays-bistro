{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/app.html",
      "dest": "/app.html"
    },
    {
      "src": "/login.html",
      "dest": "/login.html"
    },
    {
      "src": "/signup.html",
      "dest": "/signup.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "VITE_SPOONACULAR_API_KEY": "@spoonacular_api_key",
    "VITE_API_BASE_URL": "https://api.spoonacular.com",
    "VITE_APP_NAME": "SurChef",
    "VITE_APP_VERSION": "1.0.0"
  }
}
